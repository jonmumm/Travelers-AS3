	</s:states>

	<s:Rect width="100%" height="58" radiusX="10" radiusY="10">
		<s:fill>
			<s:SolidColor color="0x0D4B77" />
		</s:fill>
		<s:filters>
			<filters:ButtonDropShadowFilter />
		</s:filters>
	</s:Rect>
	<s:Label width="100%" id="labelDisplay" color="#ffffff" fontFamily="Helvetica" fontSize="33.044" fontWeight="bold" kerning="off" lineHeight="200%" textAlign="center" trackingRight="-2%" y="16" />
